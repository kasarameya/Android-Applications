<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/teal_A700"
    tools:context="com.tcs.remindmeapplication.activities.RM_TaskDetails_Activity"
    >
    <include layout="@layout/rm_my_toolbar_layout"
        android:id="@+id/toolbar_taskDetails" />
    <TextView
        android:layout_width="300dp"
        android:layout_height="35dp"
        android:text=" Your Task Details"
        android:gravity="center"
        android:padding="8dp"
        android:textStyle="bold"
        android:textColor="@color/teal_900"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/tv_tast_heading"

        />
    <TextView
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:padding="8dp"
        android:textColor="@color/teal_900"
        android:text="Task Title"
        android:id="@+id/textView7"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/friend_detail_text_background"

        />

    <TextView
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:text="Task Title"
        android:padding="8dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/tv_taskDetails_taskTitle"
        android:layout_toRightOf="@+id/textView7"
         />
    <ImageButton
        android:id="@+id/verticalline_7"
        android:layout_width="1dip"
        android:layout_height="25dp"
        android:layout_marginTop="125dp"
        android:adjustViewBounds="false"
        android:background="@android:color/transparent"
        android:clickable="false"
        android:contentDescription=""
        android:src="@color/teal_A700"
        android:layout_marginLeft="118dp"/>

    <TextView
        android:layout_width="115dp"
        android:padding="5dp"
        android:layout_height="50dp"
        android:text="Task Description"
        android:textColor="@color/teal_900"
        android:id="@+id/textView8"
        android:background="@drawable/friend_detail_text_background"
        android:layout_below="@+id/textView7"
        android:layout_marginLeft="10dp"
        />

    <TextView
        android:layout_width="185dp"
        android:layout_height="50dp"
        android:text="Task Description"
        android:padding="8dp"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/tv_taskDetails_taskDescription"
        android:layout_below="@id/tv_taskDetails_taskTitle"
        android:layout_toRightOf="@id/textView8"/>
    <ImageButton
        android:id="@+id/verticalline_8"
        android:layout_width="1dip"
        android:layout_height="25dp"
        android:layout_marginTop="170dp"
        android:adjustViewBounds="false"
        android:background="@android:color/transparent"
        android:clickable="false"
        android:contentDescription=""
        android:src="@color/teal_A700"
        android:layout_marginLeft="118dp"/>

    <TextView
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:text="Date"
        android:textColor="@color/teal_900"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/textView9"
        android:padding="8dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@id/textView8"/>

    <TextView
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:text="Date"
        android:padding="8dp"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/tv_taskDetails_date"
        android:layout_toRightOf="@id/textView9"
        android:layout_below="@id/tv_taskDetails_taskDescription"/>
    <ImageButton
        android:id="@+id/verticalline_9"
        android:layout_width="1dip"
        android:layout_height="25dp"
        android:layout_marginTop="215dp"
        android:adjustViewBounds="false"
        android:background="@android:color/transparent"
        android:clickable="false"
        android:contentDescription=""
        android:src="@color/teal_A700"
        android:layout_marginLeft="118dp"/>
    <TextView
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:text="Time"
        android:padding="8dp"
        android:textColor="@color/teal_900"
        android:id="@+id/textView10"
        android:background="@drawable/friend_detail_text_background"
       android:layout_below="@id/textView9"
        android:layout_marginLeft="10dp"
        />

    <TextView
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:text="Time"
        android:padding="8dp"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/tv_taskDetails_time"
        android:layout_below="@id/tv_taskDetails_date"
        android:layout_toRightOf="@id/textView10"
        />
    <ImageButton
        android:id="@+id/verticalline_10"
        android:layout_width="1dip"
        android:layout_height="25dp"
        android:layout_marginTop="260dp"
        android:adjustViewBounds="false"
        android:background="@android:color/transparent"
        android:clickable="false"
        android:contentDescription=""
        android:src="@color/teal_A700"
        android:layout_marginLeft="118dp"/>
    <TextView
        android:layout_width="110dp"
        android:layout_height="70dp"
        android:text="Ringtone"
        android:textColor="@color/teal_900"
        android:padding="8dp"
        android:id="@+id/textView11"
        android:layout_below="@+id/textView10"
        android:layout_marginLeft="10dp"
        android:background="@drawable/friend_detail_text_background"
        />

    <TextView
        android:layout_width="190dp"
        android:layout_height="70dp"
        android:text="Ringtone"
        android:padding="8dp"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/tv_taskDetails_ringtone"
        android:layout_below="@+id/tv_taskDetails_time"
        android:layout_alignLeft="@+id/tv_taskDetails_time"
        android:layout_alignStart="@+id/tv_taskDetails_time" />

    <ImageButton
        android:id="@+id/verticalline_11"
        android:layout_width="1dip"
        android:layout_height="30dp"
        android:layout_marginTop="302dp"
        android:adjustViewBounds="false"
        android:background="@android:color/transparent"
        android:clickable="false"
        android:contentDescription=""
        android:src="@color/teal_A700"
        android:layout_marginLeft="118dp"/>
    <TextView
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:text="Status"
        android:textColor="@color/teal_900"
        android:padding="8dp"
        android:id="@+id/textView12"
        android:layout_below="@+id/tv_taskDetails_ringtone"
        android:layout_marginLeft="10dp"

        android:background="@drawable/friend_detail_text_background"
        />
    <TextView
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:text="Status"
        android:padding="8dp"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/tv_taskDetails_Status"
        android:layout_below="@+id/tv_taskDetails_ringtone"
        android:layout_alignLeft="@+id/tv_taskDetails_ringtone"
        android:layout_alignStart="@+id/tv_taskDetails_ringtone" />

    <ImageButton
        android:id="@+id/verticalline_12"
        android:layout_width="1dip"
        android:layout_height="25dp"
        android:layout_marginTop="355dp"
        android:adjustViewBounds="false"
        android:background="@android:color/transparent"
        android:clickable="false"
        android:contentDescription=""
        android:src="@color/teal_A700"
        android:layout_marginLeft="118dp"/>
    <TextView
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:text="Vibration"
        android:padding="8dp"
        android:textColor="@color/teal_900"
        android:id="@+id/textView13"
        android:layout_below="@+id/tv_taskDetails_Status"
        android:layout_marginLeft="10dp"
        android:background="@drawable/friend_detail_text_background"
        />
    <TextView
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:text="Vibration"
        android:padding="8dp"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/tv_taskDetails_VIbration"
        android:layout_below="@+id/tv_taskDetails_Status"
        android:layout_alignLeft="@+id/tv_taskDetails_Status"
        android:layout_alignStart="@+id/tv_taskDetails_Status" />
    <ImageButton
        android:id="@+id/verticalline_13"
        android:layout_width="1dip"
        android:layout_height="25dp"
        android:layout_marginTop="400dp"
        android:adjustViewBounds="false"
        android:background="@android:color/transparent"
        android:clickable="false"
        android:contentDescription=""
        android:src="@color/teal_A700"
        android:layout_marginLeft="118dp"/>
    <TextView
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:text="Notification"
        android:padding="8dp"
        android:textColor="@color/teal_900"
        android:id="@+id/textView14"
        android:layout_below="@+id/tv_taskDetails_VIbration"
        android:layout_marginLeft="10dp"
        android:background="@drawable/friend_detail_text_background" />
    <TextView
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:text="Notification"
        android:padding="8dp"
        android:background="@drawable/friend_detail_text_background"
        android:id="@+id/tv_taskDetails_notification"
        android:layout_below="@+id/tv_taskDetails_VIbration"
        android:layout_alignLeft="@+id/tv_taskDetails_VIbration"
        android:layout_alignStart="@+id/tv_taskDetails_VIbration" />
    <ImageButton
        android:id="@+id/verticalline_14"
        android:layout_width="1dip"
        android:layout_height="25dp"
        android:layout_marginTop="440dp"
        android:adjustViewBounds="false"
        android:background="@android:color/transparent"
        android:clickable="false"
        android:contentDescription=""
        android:src="@color/teal_A700"
        android:layout_marginLeft="118dp"/>
</RelativeLayout>
